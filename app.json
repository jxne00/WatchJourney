{
  "expo": {
    "name": "WatchJourney",
    "slug": "WatchJourney",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/custom-splash.png",
